    <main>


        <div class="container mt-5">

           

            <form action="/validar" method="post" class="formulario">

                <h2> REGISTRAR PERSONA</h2>

                <input class="coso" type="number" name="ide" id="ide" placeholder="cedula">

                <input class="coso" type="text" name="nombre1" id="nombre1" placeholder="nombre1"> 

                <input class="coso" type="text" name="nombre2" id="nombre2" placeholder="nombre2">

                <input class="coso" type="text" name="apellido1" id="apellido1" placeholder="apellido1"> 

                <input class="coso" type="text" name="apellido2" id="apellido2" placeholder="apellido2">

                <input class="coso" type="number" name="celular" id="celular" placeholder="celular"> 

                <input class="coso" type="number" name="telefono" id="telefono" placeholder="telefono">

                <input class="coso" type="text" name="direccion" id="direccion " placeholder="direccion">

                <input class="botons" type="submit" value="registrar" placeholder="registrar">
            </form>



        </div>


    </main>


    <div id="myModal" class="modal">



        <span class="close">&times;</span>

        <form class="formulario">
            <h4>Logueate bb</h4>
            <input class="coso" type="text" name="nombres" id="nombres" placeholder="Ingrese su Nombre">
            <input class="coso" type="text" name="apellidos" id="apellidos" placeholder="Ingrese su Apellido">
            <input class="coso" type="email" name="correo" id="correo" placeholder="Ingrese su Correo">
            <input class="coso" type="password" name="contrasena" id="pass" placeholder="Ingrese su Contraseña">

            <input class="botons" type="submit" value="registrar" id="registrar">
            <input class="botons" type="reset" placeholder="limpiar">


        </form>

    </div>


